<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Login Test Suite" verbose="1" >
  <test name="Method Test Cases1" >
    <classes>
       <class name="Frontend.Login">
        <methods>
         <include name="StandardLogin" />
        
      </methods>
       </class>
    </classes>
  </test>
 <test name="Method Test Cases2" >
    <classes>
       <class name="Frontend.Login">
        <methods>
       
        <include name="facebookLogin" />
      
      </methods>
       </class>
    </classes>
  </test>
   <test name="Method Test Cases3" >
    <classes>
       <class name="Frontend.Login">
        <methods>
        
        <exclude name="twitterLogin" />
      </methods>
       </class>
    </classes>
  </test>
  
</suite>